6<h1>Listing books</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Tag</th>
    <th>Content</th>
    <th>Image</th>
    <th>Reference</th>
    <th>File</th>
    <th>attach</th>
    <th></th>
  </tr>

<% @books.each do |book| %>
  <tr>
    <td><%= book.Title %></td>
    <td><%= book.Tag %></td>
    <td><%= book.Content %></td>
    <td><%= book.Image %></td>
    <td><%= book.Reference %></td>
    <td><%= book.File %></td>
    <td><%= book.attach %></td>
    <td><%= link_to 'Show', book %></td>
    <td><%= link_to 'Edit', edit_book_path(book) %></td>
    <td><%= link_to 'Destroy', book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Book', new_book_path %>
